package()
{
  install -Dm644 "${srcdir}/license.txt"\
    "${pkgdir}/usr/share/license/smisioto/LICENSE"

  install -dm755 "${pkgdir}/usr/share/kicad/modules"
  install -m644 "${srcdir}/"*.mod "${pkgdir}/usr/share/kicad/modules"

  for subdir in $(find "${srcdir}/walter/"* -type d); do
    _dname="$(basename $subdir)"
    install -dm755 "${pkgdir}/usr/share/kicad/modules/packages3d/${_dname}"
    install -m644 "${subdir}/"* \
      "${pkgdir}/usr/share/kicad/modules/packages3d/${_dname}"
  done
}

